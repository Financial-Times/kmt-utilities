{
    "ecmaFeatures" : {
        "generators": true
    },
    "rules": {
        "curly": 2,
        "eqeqeq": 1,
        "space-before-blocks": [2, "always"],
        "no-extend-native": 2,
        "wrap-iife": 2,
        "new-cap": [1, {
            "newIsCap": true,
            "capIsNew": false
        }],
        "no-caller": 2,
        "no-irregular-whitespace": 1,
        "no-trailing-spaces": 2,
        "no-multi-str": 0,
        "dot-notation": 2,
        "no-unused-vars": [2, { "vars": "all", "args": "none" }],
        "valid-jsdoc": 1,
        "no-undef": 2,
        "no-use-before-define": 2,
        "no-multi-spaces": 1,
        "no-multiple-empty-lines": 1,
        "one-var": [2, "never"],
        "quotes": [1, "single", "avoid-escape"],
        "strict": 0,
        "indent": 0,
        "no-new": 0,
        "no-unused-expressions": 1,
        "one-var": 0,
        "no-underscore-dangle": 0
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "mocha": true
    }
}
